{
  "extends": ["@project/eslint-config"],
  "overrides": [
    {
      "files": ["**/*.ts", "**/*.tsx"],
      "parserOptions": {
        "project": "./tsconfig.json"
      },
      "rules": {
        "@typescript-eslint/no-unused-vars": [
          "warn",
          { "varsIgnorePattern": "Resolver" }
        ]
      }
    }
  ]
}
