{
  "name": "yestheory.family",
  "private": true,
  "scripts": {
    "lint": "yarn eslint:fix && yarn prettier:fix",
    "eslint:check": "yarn workspaces foreach --all --parallel run eslint:check",
    "eslint:fix": "yarn workspaces foreach --all --parallel run eslint:fix",
    "prettier:check": "yarn workspaces foreach --all --parallel run prettier:check",
    "prettier:fix": "yarn workspaces foreach --all --parallel run prettier:fix"
  },
  "workspaces": [
    "packages/*"
  ],
  "resolutions": {
    "react-test-renderer": "18.2.0",
    "ts-node": "10.9.1"
  },
  "packageManager": "yarn@4.0.2"
}
