{
  "name": "yestheory.family",
  "private": true,
  "scripts": {
    "e2e:open": "yarn workspace @yestheory.family/web e2e:open",
    "e2e:run": "yarn workspace @yestheory.family/web e2e:run",
    "eslint:check": "yarn workspaces foreach run lint",
    "eslint:write": "yarn workspaces foreach run lint --fix",
    "format:check": "prettier --check packages",
    "format:write": "prettier --write packages",
    "server:dev": "yarn workspace @yestheory.family/server start",
    "server:generate": "yarn workspace @yestheory.family/server packages/server/prisma generate",
    "storybook": "yarn workspace @yestheory.family/ui storybook",
    "test": "yarn workspaces foreach run test",
    "web:dev": "yarn workspace @yestheory.family/web dev",
    "web:generate": "yarn workspace @yestheory.family/web codegen"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "prettier": "2.6.2"
  },
  "resolutions": {
    "@graphql-codegen/typescript": "2.7.1",
    "@graphql-codegen/typescript-operations": "2.5.1",
    "react-test-renderer": "18.1.0"
  },
  "packageManager": "yarn@3.2.1"
}
